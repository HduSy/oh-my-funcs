{
  "name": "oh-my-utils",
  "version": "1.0.1",
  "description": "前端开发常用功能函数，拿来即用",
  "type": "module",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "jsnext:main": "./lib/index.esm.js",
  "browser": "./lib/index.umd.js",
  "typings": "./lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HduSy/oh-my-utils"
  },
  "author": "HduSy <bilisantu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm clean && rollup -c -w --configPlugin @rollup/plugin-typescript",
    "build": "pnpm clean && rollup -c --configPlugin @rollup/plugin-typescript",
    "clean": "rm -rf lib",
    "release": "commit-and-tag-version",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/HduSy/oh-my-utils/issues"
  },
  "homepage": "https://github.com/HduSy/oh-my-utils",
  "keywords": [
    "utils",
    "function",
    "tools"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/hammerjs": "^2.0.45",
    "@types/smooth-scroll": "^16.1.4",
    "@types/smoothscroll-polyfill": "^0.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "lz-string": "^1.5.0",
    "rollup": "^4.9.5",
    "smooth-scroll": "^16.1.3",
    "smoothscroll-polyfill": "^0.4.4"
  }
}
